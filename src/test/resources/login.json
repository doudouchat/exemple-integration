{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"additionalProperties": false,
	"required": [
		"username",
		"password",
		"id"
	],
	"properties": {
		"username": {
			"type": "string"
		},
		"id": {
			"type": "string"
		},
		"password": {
			"type": "string",
			"writeOnly": true
		},
		"disabled": {
			"type": "boolean"
		}
	}
}